#include <stdio.h>
int prime(int n){
    for(int i=2;i<n;i++) if(n%i==0) return 0;
    return 1;
}
int main(){
    int n; scanf("%d",&n);
    for(int i=2;i<n;i++){
        if(prime(i) && prime(n-i)){
            printf("%d = %d + %d",n,i,n-i);
            return 0;
        }
    }
    printf("Not possible");
}