#include <stdio.h>
int main() {
    int a[] = {9, 2, 5, 1}, n = 4, min, t;

    for(int i=0;i<n-1;i++){
        min = i;
        for(int j=i+1;j<n;j++)
            if(a[j] < a[min])
                min = j;
        t=a[i]; a[i]=a[min]; a[min]=t;
    }

    for(int i=0;i<n;i++) printf("%d ", a[i]);
}